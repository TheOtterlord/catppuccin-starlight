{
	"name": "starlight-theme-catppuccin",
	"version": "0.0.1",
	"type": "module",
	"exports": {
		".": "./index.ts",
		"./styles/*": "./styles/*",
		"./themes/*": "./themes/*"
	},
	"files": [
		"index.ts",
		"themes"
	],
	"keywords": [
		"withastro"
	],
	"scripts": {
		"dev": "nodemon --watch \"./templates/*\" -e tera --exec \"pnpm build\"",
		"build": "whiskers --flavor mocha templates/dark.tera && whiskers --flavor macchiato templates/dark.tera && whiskers --flavor frappe templates/dark.tera && whiskers --flavor latte templates/light.tera"
	},
	"devDependencies": {
		"astro": "^4.15.3",
		"nodemon": "^3.1.7"
	},
	"peerDependencies": {
		"@astrojs/starlight": "^0.28.0",
		"astro": "^4.0.0"
	},
	"dependencies": {
		"@astrojs/starlight": "catalog:",
		"typescript": "^5.5.4"
	}
}
